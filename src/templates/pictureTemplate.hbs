{{#each this}}
<li>
    <div class="photo-card">
        {{!-- <a id="image-link" href="{{fullHDURL}}"> --}}
            <img class="picture" src='{{webformatURL}}' alt="{{tags}}" />
            {{!-- </a> --}}
        {{!-- <img class="picture" src='{{webformatURL}}' alt="{{tags}}" /> --}}
        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                {{downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}